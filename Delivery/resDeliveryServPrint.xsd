<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:include schemaLocation="common.xsd"/>

    <xs:element name="RK7QueryResult">
        <xs:annotation>
            <xs:documentation xml:lang="ru">[Касса, Кассовый сервер] xml-ответ: DeliveryServPrint</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RK7QueryResult">
                    <xs:attribute name="version" type="xs:nonNegativeInteger">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Версия заказа</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="crc32" type="xs:int" use="required">
                      <xs:annotation>
                        <xs:documentation xml:lang="ru">Контрольная сумма по содержимому заказа</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
</xs:schema>
